<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ms.tet">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:name=".App"
        android:theme="@style/Theme.Teris">
        <activity android:name="com.ms.tet.RecordsActivity"></activity>
        <activity android:name="com.ms.tet.GameOver" />
        <activity android:name="com.ms.tet.Table" />
        <activity android:name="com.ms.tet.Config" />
        <activity android:name="com.ms.tet.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>